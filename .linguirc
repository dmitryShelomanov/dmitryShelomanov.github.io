{
  "sourceLocale": "ru",
  "locales": [
    "ru",
    "en"
  ],
  "compileNamespace": "es",
  "catalogs": [
    {
      "path": "src/i18n/locales/{locale}",
      "include": [
        "src/**/*"
      ],
      "exclude": []
    }
  ],
  "format": "lingui",
  "extractBabelOptions": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
